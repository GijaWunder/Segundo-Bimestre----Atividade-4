programa
{
	
	funcao inicio()
	{
		inteiro numero1
		inteiro numero2

		escreva("Me informe dois números: \n")
		leia(numero1, numero2)

		escreva("\nOs números entre ", numero1, " e ", numero2, " são: ")

		se(numero1 < numero2){
		para(inteiro i=numero1+1; i < numero2; i++){
			escreva(i, " - ")
		}
		}
		se(numero1 > numero2){
		para(inteiro i=numero1-1; i > numero2-2; i--){
		escreva(i, " - ")
		}
	}
}
}
